    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoaoHamerski/cli-template-generator.git"
  },
  "author": "João Antônio Hamerski Copetti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JoaoHamerski/cli-template-generator/issues"
  },
  "homepage": "https://github.com/JoaoHamerski/cli-template-generator#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "prettier": "3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.12",
    "commander": "^12.0.0",
    "enquirer": "^2.4.1",
    "handlebars": "^4.7.8",
    "lodash-es": "^4.17.21"
